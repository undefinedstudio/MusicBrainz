{
    "name": "undefinedstudio/musicbrainz",
    "type": "library",
    "description": "A PHP library for accessing the MusicBrainz API",
    "keywords": ["musicbrainz", "library", "php", "api"],
    "homepage": "https://github.com/undefinedstudio/musicbrainz",
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "*",
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "rmccue/requests": "Allows you to use the RequestsHttpAdapter. You must install one of the supported adapters."
    },
    "autoload": {
        "psr-0": {
            "MusicBrainz": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
